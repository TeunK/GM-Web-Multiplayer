//Required Level
draw_text(view_xview[0]+420,view_yview[0]+60,string("Req Lvl: ")+string(obj_Controller.Questlevel[page]));

//Quest Status
if obj_Controller.Questactive[page] = 0
{draw_text(view_xview[0]+420,view_yview[0]+70,string("Not Yet Started"));}
else if obj_Controller.Questactive[page] = 1
{draw_text(view_xview[0]+420,view_yview[0]+70,string("In Progress"));}
else if obj_Controller.Questactive[page] = 2
{draw_text(view_xview[0]+420,view_yview[0]+70,string("Completed!"));}

//Quest Location
draw_text(view_xview[0]+420,view_yview[0]+80,string("Location: ")+string(obj_Controller.Questlocation[page]));

//Line to details
