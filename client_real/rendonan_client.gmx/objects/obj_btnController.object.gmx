            <kind>1</kind>
            <string>//Scrolling through menu
timer-=1
if timer&lt;=0 {timer=0}

if (keyboard_check_pressed(ord('S')) || keyboard_check_pressed(vk_down))
   {
    if timer &lt;=0 {if selected="Play" {selected="Credits"; timer=2}}
    if timer &lt;=0 {if selected="Credits" {selected="Exit"; timer=2}}
    if timer &lt;=0 {if selected="Exit" {selected="Play"; timer=2}}
   }
   


if (keyboard_check_pressed(ord('W')) || keyboard_check_pressed(vk_up))
   {
    if timer &lt;=0 {if selected="Play" {selected="Exit"; timer=2}}
    if timer &lt;=0 {if selected="Credits" {selected="Play"; timer=2}}
    if timer &lt;=0 {if selected="Exit" {selected="Credits"; timer=2}}
   }

if instance_exists(obj_btnPlay) &amp;&amp; instance_exists(obj_btnCredits) &amp;&amp; instance_exists(obj_btnExit)
{
if selected="Play"    {obj_btnPlay.mouse=1; obj_btnCredits.mouse=0; obj_btnExit.mouse=0; }
if selected="Load"    {obj_btnPlay.mouse=0; obj_btnCredits.mouse=0; obj_btnExit.mouse=0; }
if selected="Credits" {obj_btnPlay.mouse=0; obj_btnCredits.mouse=1; obj_btnExit.mouse=0; }
if selected="Exit"    {obj_btnPlay.mouse=0; obj_btnCredits.mouse=0; obj_btnExit.mouse=1; }
}
</string>
