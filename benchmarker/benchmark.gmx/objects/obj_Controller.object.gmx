            <string>while(wn_receive_message(server_socket))
{
    var msg_type=wn_msgtype("S2C",wn_read_byte());
    var sender_name = wn_read_string();
    
    switch(msg_type)
    {
        case "confirm_client_id":
            //respond with our username
            var senderid
            senderid = ds_map_find_value(nodes,sender_name)
            if instance_exists(senderid)
            {
                with (senderid)
                {
                    var server_socket = obj_Controller.server_socket
                    wn_clear_buffer();
                    wn_write_byte(wn_msgtype("C2S", "initial_client_details"));
                    wn_write_string(client_name);
                    wn_send_message(server_socket)
                }
            }
            break;
            
        case "chat_message":
            var senderid
            senderid = ds_map_find_value(nodes,sender_name)
            var chat_message = wn_read_string();
            if instance_exists(senderid)
            {
                with (senderid)
                {                    
                    obj_Chatbox.chat_contents = add_line(obj_Chatbox.chat_contents, sender_name, chat_message, c_white);
                }
            }
            
            break;
        
        case "ping":
            var senderid
            senderid = ds_map_find_value(nodes,sender_name)
            var ping_starttime = wn_read_string();
            if instance_exists(senderid)
            {
                with (senderid)
                {
                    //store time (first shift each ping in history by 1)
                    for (i=ping_history_limit-1; i&gt;=0; i-=1)
                    {
                        ping[i+1] = ping[i]
                        instances[i+1] = instances[i]
                    }
                    ping[0] = (string(get_timer()/1000) - ping_starttime)
                    instances[0] = instance_number(obj_Network_controller)
                }
            }
            
            break;
            
        case "client_count":
            client_count = wn_read_byte();
                        
            break;
            
        case "request_count":
            request_count = wn_read_string();
            
            break;
    }
}
